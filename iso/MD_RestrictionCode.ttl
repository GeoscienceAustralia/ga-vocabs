    skos:prefLabel "trademark" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/unrestricted> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "no constraints exist"^^xsd:string ;
