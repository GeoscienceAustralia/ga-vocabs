<http://def.isotc211.org/iso19115/MD_RestrictionCode/confidential> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition """not available to the public
NOTE Contains information that could be prejudicial to a commercial, industrial, or national interest."""^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "confidential" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/copyright> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "exclusive right to the publication, production, or sale of the rights to a literary, dramatic, musical, or artistic work, or to the use of a commercial print or label, granted by law for a specified period of time to an author, composer, artist, distributor"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "copyright" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/in-confidence> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "with trust"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "in-confidence" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/intellectualPropertyRights> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "rights to financial benefit from and control of distribution of non-tangible property that is a result of creativity"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "intellectual Property Rights" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/licence> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "formal permission to do something"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "licence" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/licenceDistributor> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "formal permission required for a person or an entity to commercialize or distribute the resource"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "licence Distributor" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/licenceEndUser> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "formal permission required for a person or an entity to use the resource and that may differ from the person that orders or purchases it"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "licence End User" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/licenceUnrestricted> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "formal permission not required to use the resource"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "licence Unrestricted" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/otherRestrictions> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "limitation not listed"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "other Restrictions" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/patent> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "government has granted exclusive right to make, sell, use or license an invention or discovery"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "patent" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/patentPending> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "produced or sold information awaiting a patent"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "patent Pending" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/private> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "protects rights of individual or organisations from observation, intrusion, or attention of others"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "private" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/restricted> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "withheld from general circulation or disclosure"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "restricted" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/sensitiveButUnclassified> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "although unclassified, requires strict controls over its distribution."^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "sensitive But Unclassified" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/statutory> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "prescribed by law"^^xsd:string ;
    skos:inScheme cs: ;
    skos:prefLabel "statutory" ;
    skos:topConceptOf cs: .

<http://def.isotc211.org/iso19115/MD_RestrictionCode/trademark> a <http://def.isotc211.org/iso19115/-1/2014/ConstraintInformation#MD_RestrictionCode>,
        skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "a name, symbol, or other device identifying a product, officially registered and legally restricted to the use of the owner or manufacturer"^^xsd:string ;
