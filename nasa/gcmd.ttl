    skos:inScheme cs:  ;
    skos:prefLabel "AM INDEX"@en .

gcmd:b32ca9df-f981-4696-bf97-c190175e47b7 a skos:Concept ;
    skos:definition "Subphylum of Arthropoda distinguished by chelicerae appendages near the mouth to aid in feeding and only two main body regions, the cephalothorax (prosoma) and the abdomen (opisthosoma)"@en ;
    skos:inScheme cs:  ;
    skos:narrower gcmd:973bd2bd-c201-4e8c-8c86-d2e849298310 ;
    skos:prefLabel "CHELICERATES"@en .

gcmd:b32eb466-3fc1-4a03-8011-99002f5591cb a skos:Concept ;
    skos:altLabel "GEOLOGICAL HAZARDS MAPPING"@en ;
    skos:definition "Mapping of ‚Äãhazards originating from the solid earth. Examples include earthquakes, tsunamis, and volcanic activity."@en ;
