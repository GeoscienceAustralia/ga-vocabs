    skos:inScheme cs:  ;
    skos:prefLabel "LAVA PLAIN"@en .

gcmd:dc2088f9-ffb7-41c3-b0a1-856ecfec89de a skos:Concept ;
    skos:definition "Refers to software, tools, and activities that support formal or classroom-based education, as well as informal education that occurs outside of the classroom."@en ;
    skos:inScheme cs:  ;
