        gcmd:882d9a10-713c-4b58-8c7b-d9af086115a3,
        gcmd:8cbdfa00-852c-452d-8013-86145ad318c8,
        gcmd:990016e0-f247-4c36-8a17-f50e792a964a,
        gcmd:9abe9fdb-59f3-4bd6-b24f-b9b7e46eae7c,
        gcmd:ba75172a-6965-40cf-bf3f-6c0af2f97dad,
        gcmd:e453077b-b6f3-44f0-9f3d-4408bf9a69e5 ;
    skos:prefLabel "IONOSPHERE/MAGNETOSPHERE DYNAMICS"@en .

gcmd:3a95742c-1355-40af-ac86-e24365a67b04 a skos:Concept ;
    skos:definition "The rate at which clay particles separate from one other in moist soil."@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "DISPERSION RATE"@en .

gcmd:3ab3aa92-9cca-4660-a0ed-281fff07eede a skos:Concept ;
    skos:definition "The erosion, transport, and deposition of material due to the action of the wind at or near the Earth's surface. Aeolian processes are at their most effective when the vegetation cover is discontinuous or absent."@en ;
    skos:inScheme cs:  ;
