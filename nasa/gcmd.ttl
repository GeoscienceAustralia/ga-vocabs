    skos:inScheme cs:  ;
    skos:narrower gcmd:14d972b3-a587-4994-a021-f1e620b02341,
        gcmd:8c6adb44-54c5-42f1-ae19-602e248ff9d9,
        gcmd:d73ed320-cd5b-4994-a26a-dac5a2fc394f ;
    skos:prefLabel "BIOGEOCHEMICAL PROCESSES"@en .

gcmd:b498a5cb-f77d-4485-8174-81dec28cee0e a skos:Concept ;
    skos:definition "A natural flow of water from the sub-surface to the surface. Usually occurs when the water table intersects the Earth's surface."@en ;
