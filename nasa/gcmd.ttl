gcmd:7bc06198-5546-40f5-97ac-b7b5b5503cfc a skos:Concept ;
    skos:inScheme cs:  ;
    skos:prefLabel "POPULATION ABUNDANCE"@en .

gcmd:7bfdbe8d-3945-4678-a90b-d2251f973955 a skos:Concept ;
