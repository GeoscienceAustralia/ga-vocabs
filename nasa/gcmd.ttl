    skos:inScheme cs:  ;
    skos:narrower gcmd:9273a48a-7ca4-4f1a-9347-7f6599b5a7e3,
        gcmd:b6782a30-639e-4d70-8290-81683d248b1f,
        gcmd:bc60fbb8-f9e9-492e-9acf-0f47345cedf2 ;
    skos:prefLabel "BIVALVES"@en .

gcmd:7da95c01-4b39-437e-a8d4-fd572e43f693 a skos:Concept ;
    skos:definition "Wetlands is a term for a broad group of wet habitats.  They are transitional lands between terrestrial and aquatic ecosystems where the lands may be permanently or intermittently water covered."@en ;
    skos:inScheme cs:  ;
    skos:narrower gcmd:0e1f3f95-58b5-4f10-b239-850c66ed55ff,
        gcmd:1af675ae-9a65-4d91-970e-a8b9fcce0232,
