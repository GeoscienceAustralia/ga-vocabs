    skos:inScheme cs:  ;
    skos:prefLabel "CHEMICAL SOLUTION"@en .

gcmd:bb8c48bc-a36e-4f7e-afda-3244b058bc9c a skos:Concept ;
    skos:definition "Pertaining to the measuerment of a large mass of snow, ice, soil, rock, falling rapidly from heights far above a lowland area."@en ;
