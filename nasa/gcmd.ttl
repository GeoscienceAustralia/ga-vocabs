        gcmd:8de8b909-8fcb-4ed7-9df3-37f9dd54054f ;
    skos:prefLabel "FIRE ADVISORIES"@en .

gcmd:7f8a1613-67b0-4d6a-a9ad-89097c27a052 a skos:Concept ;
    skos:definition """The ability of some microorganisms to utilize the chemical energy contained
in certain inorganic chemicals such as hydrogen sulfide for the production
of organic material."""@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "CHEMOSYNTHESIS"@en .

gcmd:7f94b0e5-edc6-4724-bd84-404896e09afe a skos:Concept ;
    skos:definition "Air temperature measured within the the bottom layer (atmospheric boundary layer) of the troposphere that is in contact with the surface of the earth."@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "BOUNDARY LAYER TEMPERATURE"@en .

gcmd:7f95ceda-09fd-4ee3-9f30-bf38bf831e12 a skos:Concept ;
    skos:inScheme cs:  ;
    skos:prefLabel "MONSOON ONSET/INTENSITY"@en .

gcmd:7fa9d5ef-690e-4daf-8503-363b6b1cb6e4 a skos:Concept ;
    skos:definition "A set of deposited sedimentary beds that reflects the depositional environment of those beds and the geologic history of a region. A stratigraphic sequence is a chronologic succession of sedimentary rocks from older below to younger above without interruption."@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "STRATIGRAPHIC SEQUENCE"@en .

gcmd:7fdc339e-017f-4e4b-89a3-12e441a40bad a skos:Concept ;
