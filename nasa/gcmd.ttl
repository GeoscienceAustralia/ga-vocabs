    skos:prefLabel "LAND RESOURCES"@en .

gcmd:c79453a3-ed2f-4ec4-9298-bf9fd11d08eb a skos:Concept ;
    skos:definition "The Concentration of element Lead (Pb) in the atmosphere."@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "LEAD"@en .

