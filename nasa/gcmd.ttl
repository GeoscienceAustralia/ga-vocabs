gcmd:16d0abc3-8f75-4974-bdb4-df09a04bcfa3 a skos:Concept ;
    skos:definition """Services that provide links to subscriptions to
scientific/environmental Internet newsletters and notifications."""@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "SUBSCRIPTION SERVICES"@en .

gcmd:16e33a06-ae4b-48cd-be7f-ad44f3dfd23b a skos:Concept ;
