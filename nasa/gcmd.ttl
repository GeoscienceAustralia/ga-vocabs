        gcmd:e56bcf72-f331-4545-948f-73fe0193b1bd ;
    skos:prefLabel "MAXIMUM/MINIMUM TEMPERATURE"@en .

gcmd:517e2978-223e-42a2-b889-9c60f7099859 a skos:Concept ;
    skos:definition "Tube shaped segmented worm in Phylum Annelida"@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "EARTHWORMS"@en .

gcmd:5181e50a-b1d2-41b1-bde3-fd9b4da9b1bf a skos:Concept ;
    skos:definition """Of, or pertaining to, the outer perimeter of a glacier, particularly to the\r
fringe areas surrounding the great continental glaciers of the geologic ice\r
ages.  Thus, periglacial weathering is said to have produced certain\r
characteristic land forms."""@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "PERIGLACIAL PROCESSES"@en .

gcmd:519a7291-55a2-44a4-8f01-ca7742ff69cc a skos:Concept ;
