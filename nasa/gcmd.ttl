gcmd:e631c681-5dad-48b2-83ce-943a1f0df47a a skos:Concept ;
    skos:definition "Small ten-footed aquatic crustaceans in Phylum Arthropoda"@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "DECAPODS"@en .

gcmd:e63844c1-015c-4776-b01c-e3e7d5dd3d0c a skos:Concept ;
    skos:definition "An index that is determined using visible and near-IR channels on AVHRR. Denotes relative chlorophyll content of vegetation during daylight conditions, and is thus used to monitor drought conditions."@en ;
    skos:inScheme cs:  ;
    skos:narrower gcmd:7b43eda3-899a-4afa-89be-2dbe527834c2 ;
    skos:prefLabel "VEGETATION INDEX"@en .

gcmd:e657b41f-4aa0-4816-b3c8-5b477812a0bc a skos:Concept ;
    skos:definition "Karst Hydrology refers to the extensive dissolution of rock has led to the development of subterranean channels through which groundwater flows in conduits (enclosed or semi-enclosed channels). These conduits can vary in size from slightly enlarged cracks to tunnels many meters in diameter and many kilometers in length."@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "KARST HYDROLOGY"@en .

gcmd:e65cfeec-0da2-40d8-b80d-1c74d1a498fc a skos:Concept ;
