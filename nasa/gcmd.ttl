        gcmd:f2044dcf-40da-4fcc-97ab-914343d885a5 ;
    skos:prefLabel "ARTHROPODS"@en .

gcmd:bb891ee1-6c7b-4ec0-b2fa-6fb67a2df2a3 a skos:Concept ;
