        gcmd:b5a681af-5005-4182-922e-528ec8d514f1,
        gcmd:e61fcc9f-bdb6-4dbc-94f2-52c4c64b6df9 ;
    skos:prefLabel "TROPICAL CYCLONE TRACK"@en .

gcmd:10a9c153-f37d-48fe-920d-c790d946ab07 a skos:Concept ;
    skos:definition """The circulatory vertical motion in a fluid medium (ocean water) owing to
the variation of its density and the action of gravity."""@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "CONVECTION"@en .

gcmd:10b1872b-4a48-4360-a449-388e8988bca9 a skos:Concept ;
    skos:definition """A continuous sheet of land ice that covers a very large area and moves outward\r
in many directions. \r
This type of ice mass is so thick as to mask the land surface contours, in\r
contrast to the smaller and thinner highland ice. The continental glacier of\r
Greenland is sometimes called the Inland Ice. This term is often used to\r
describe the great ice masses that characterized the ice ages."""@en ;
    skos:inScheme cs:  ;
    skos:narrower gcmd:6d6a2b61-5d2c-4ec1-a164-34000f481588,
        gcmd:94402f47-38ea-4798-98da-ea17599e092f ;
    skos:prefLabel "ICE SHEETS"@en .

gcmd:10ec2826-c4ac-4373-ad05-9bb4eb35b360 a skos:Concept ;
    skos:definition "The process by which surface sediment is incorporated into a fluid flow (such as air, water or even ice ) as part of the operation of erosion."@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "ENTRAINMENT"@en .

gcmd:11175bd5-ee63-4b13-aa03-bc5500a458c2 a skos:Concept ;
    skos:definition "Geographical features found on coastlines and created by long shore drift. Made out of sand and shingle, and later stabilized by vegetation, cuspate forelands are triangular-shaped accretions and extend seawards."@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "CUSPATE FORELANDS"@en .

gcmd:112e71ec-c0a1-49a8-82d7-bcb317b45860 a skos:Concept ;
    skos:altLabel "VEGETATION"@en ;
    skos:inScheme cs:  ;
    skos:narrower gcmd:16329a9b-72ea-4b46-b507-2ce389c63f50,
        gcmd:1a95af83-fe1b-4cea-931d-953a4e5965e6,
