        gcmd:e987550e-d443-48eb-93eb-0bc47a62d4b4 ;
    skos:prefLabel "SURFACE WINDS"@en .

gcmd:107582ef-a356-4afa-a9a4-4e1d2200c134 a skos:Concept ;
    skos:definition "The amount of radiation incident on a given surface per unit time."@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "RADIATIVE FLUX"@en .

gcmd:108318e6-89c0-4e0d-bcad-7bfdb0df49f5 a skos:Concept ;
    skos:definition "Model configurations / inputs / outputs / logs for ancillary models"@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "MODEL LOGS"@en .

gcmd:1088e9e2-dadd-4d20-a2db-ef7df32c6d42 a skos:Concept ;
    skos:definition "The movement of solid particles (sediment), typically due to a combination of the force of gravity acting on the sediment, and/or the movement of the fluid in which the sediment is entrained. An understanding of sediment transport is typically used in natural systems, where the particles are clastic rocks (sand, gravel, boulders, etc.), mud, or clay; the fluid is air, water, or ice; and the force of gravity acts to move the particles due to the sloping surface on which they are resting."@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "SEDIMENT TRANSPORT"@en .

gcmd:108ece15-4588-4564-b803-dc1c17cf193e a skos:Concept ;
    skos:definition "Phylum Chordata. Class Mammalia. Order Artiodactyla. Family Suidae are omnivores, even toed-ungulates with a large head and a long strengthened snout. Hog is a common name for domestic pig weighing more than 120lbs."@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "HOGS/PIGS"@en .

gcmd:10a128a6-12d4-4bce-b25d-2ffc464182f4 a skos:Concept ;
    skos:definition """Any non-linear shaped opening enclosed by ice. May contain brash ice and/or\r
be covered with new ice, nilas or young ice; sub-mariners refer to these\r
as skylights. Polynyas are important in the study of the energy budget of\r
the polar ocean and local and regional climatology."""@en ;
    skos:inScheme cs:  ;
    skos:prefLabel "POLYNYAS"@en .

gcmd:10a9bb22-9119-4409-84c1-7c97ef31b1a1 a skos:Concept ;
    skos:definition "Tropical Cyclone path, route, or course."@en ;
    skos:inScheme cs:  ;
