    skos:inScheme cs: ;
    skos:prefLabel "density"@en ;
.

:dissolved-solids
    a skos:Concept ;
    skos:broader :aquifers ;
    skos:definition "Dissolved solids\" refer to any minerals, salts, metals, cations or anions dissolved in water. "@en ;
