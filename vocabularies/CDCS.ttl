    skos:inScheme cs: ;
    skos:prefLabel "transport"@en ;
.

:u-pb
    a skos:Concept ;
    skos:broader :radio-isotopes ;
    skos:definition "Radioactive isotope dating technique"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "u pb"@en ;
.

