    skos:inScheme cs: ;
    skos:prefLabel "stratigraphic holes"@en ;
.

:surfaces
    a skos:Concept ;
    skos:broader :aquifers ;
    skos:definition "Examples include potentiometric and phreatic surfaces"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "surfaces"@en ;
.

:taxonomy
    a skos:Concept ;
    skos:broader :palaeontology ;
