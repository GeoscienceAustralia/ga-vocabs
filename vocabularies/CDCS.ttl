    skos:inScheme cs: ;
    skos:prefLabel "wave velocities"@en ;
.

:aquatic
    a skos:Concept ;
    skos:broader :natural-habitat ;
    skos:definition "Pertains to both marine and freshwater ecosystems. For example, reefs"@en ;
    skos:inScheme cs: ;
    skos:narrower :benthic ;
    skos:prefLabel "aquatic"@en ;
.

:bathymetry
    a skos:Concept ;
