    skos:definition "Oil and gas production"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "hydrocarbon"@en ;
.

:hydrography
    a skos:Concept ;
    skos:broader :bathymetry ;
    skos:definition "Information related to the distribution,  mapping and physical characteristics of surficial waters. Examples include the locations of bores, lakes, dams, canals, sea levels etc,"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "hydrography"@en ;
.

:inorganic
    a skos:Concept ;
    skos:broader :geochemistry ;
