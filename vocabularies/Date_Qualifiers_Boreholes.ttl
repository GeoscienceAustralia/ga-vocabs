    skos:inScheme cs: ;
    skos:prefLabel "estimated earliest"@en ;
.

<https://pid.geoscience.gov.au/def/voc/ga/DateQualifier/estimatedLatest>
    a skos:Concept ;
    dcterms:identifier "estimatedLatest"^^xsd:token ;
