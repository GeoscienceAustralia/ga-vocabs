<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/C1> a skos:Concept ;
    skos:altLabel "C1"@en ;
    skos:definition "rich gas and no shows or milky liquid in acetone test"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Condensate indication"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/C2> a skos:Concept ;
    skos:altLabel "C2"@en ;
    skos:definition "anomalous rich gas and milky liquid in acetone test and no shows"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Strong condensate indication"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/C3> a skos:Concept ;
    skos:altLabel "C3"@en ;
    skos:definition "condensate flowed on test"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Condensate show"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/C4> a skos:Concept ;
    skos:altLabel "C4"@en ;
    skos:definition "condensate show with convincing log anomaly or other indication"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Potential condensate zone"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/C5> a skos:Concept ;
    skos:altLabel "C5"@en ;
    skos:definition "sustained condensate flow on test, or RFT & log anomaly or pressure data proving an accumulation (no economic implications)"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Proven condensate zone"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/DHI> a skos:Concept ;
    skos:altLabel "DHI"@en ;
    skos:definition "undifferentiated dhi reported from seismic."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Direct HC indicator, undif"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/DHIB> a skos:Concept ;
    skos:altLabel "DHIB"@en ;
    skos:definition "seismic shows amplitude anomaly over well"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Direct HC indicator, brt spot"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/DHIC> a skos:Concept ;
    skos:altLabel "DHIC"@en ;
    skos:definition "seismic shows shallow gas indication."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Direct HC indicator, chimney"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/DHIF> a skos:Concept ;
    skos:altLabel "DHIF"@en ;
    skos:definition "seismic shows flat spot."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Direct HC indicator, flat spot"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/G0> a skos:Concept ;
    skos:altLabel "G0"@en ;
    skos:definition "trace to high gas reading."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Gas traces"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/G1> a skos:Concept ;
    skos:altLabel "G1"@en ;
    skos:definition "anomalously high gas reading"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Gas indication"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/G2> a skos:Concept ;
    skos:altLabel "G2"@en ;
    skos:definition "anomalously high gas reading and other indication. eg from core, logs, or shakers"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Strong gas indication"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/G3> a skos:Concept ;
    skos:altLabel "G3"@en ;
    skos:definition "gas flowed on test"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Gas show"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/G4> a skos:Concept ;
    skos:altLabel "G4"@en ;
    skos:definition "gas show with convincing log anomaly or other indication"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Potential gas zone"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/G5> a skos:Concept ;
    skos:altLabel "G5"@en ;
    skos:definition "sustained gas flow on test, or RFT & log anomaly proving an accumulation (no economic implications)"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Proven gas zone"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/L0> a skos:Concept ;
    skos:altLabel "L0"@en ;
    skos:definition "stains of dead oil, no fluorescence, no cut or was not able to differentiate between oil fluorescence and mineral fluorescence."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Oil traces"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/L1> a skos:Concept ;
    skos:altLabel "L1"@en ;
    skos:definition "fluorescence or cut"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Oil indication"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/L2> a skos:Concept ;
    skos:altLabel "L2"@en ;
    skos:definition "fluorescence or cut & other oil indication. eg log anomaly"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Strong oil indication"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/L3> a skos:Concept ;
    skos:altLabel "L3"@en ;
    skos:definition "oil recovered from core, test, mud"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Oil show"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/L4> a skos:Concept ;
    skos:altLabel "L4"@en ;
    skos:definition "oil show with convincing oil anomaly"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Potential oil zone"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/L5> a skos:Concept ;
    skos:altLabel "L5"@en ;
    skos:definition "oil flow on test, or RFT & log anomaly proving an accumulation (no economic implications)"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Proven oil zone"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/NC> a skos:Concept ;
    skos:altLabel "NC"@en ;
    skos:definition "no condensate show for entire well"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "No condensate show"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/NG> a skos:Concept ;
    skos:altLabel "NG"@en ;
    skos:definition "no gas show for entire well"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "No gas show"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/NO> a skos:Concept ;
    skos:altLabel "NO"@en ;
    skos:definition "no oil show for entire well"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "No oil show"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/NR> a skos:Concept ;
    skos:altLabel "NR"@en ;
    skos:definition "MANDATORY FIELD"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "No returns - lost circulation"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/NS> a skos:Concept ;
    skos:altLabel "NS"@en ;
    skos:definition "no shows over the entire well"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "No show"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/NST> a skos:Concept ;
    skos:altLabel "NST"@en ;
    skos:definition "No shows from test/s.  This could be a single test or multiple tests over a zone."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "No shows from test/s"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/NT> a skos:Concept ;
    skos:altLabel "NT"@en ;
    skos:definition "no tests performed in the well"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "No tests"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/OOO> a skos:Concept ;
    skos:altLabel "OOO"@en ;
    skos:definition "MANDATORY FIELD"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Test equipment out of order"@en ;
    skos:topConceptOf cs: .

<http://pid.geoscience.gov.au/def/Hydrocarbon_Show/UHC> a skos:Concept ;
    skos:altLabel "UHC"@en ;
    skos:definition "MANDATORY FIELD"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Undifferentiated HC shows"@en ;
    skos:topConceptOf cs: .

