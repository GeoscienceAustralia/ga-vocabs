<http://www.odm2.org> a sdo:Organization ;
    sdo:name "Observations Data Model 2 Group" ;
    sdo:url <https://github.com/ODM2/> .

<http://vocabulary.odm2.org/specimentype/automated> a skos:Concept ;
    skos:definition "Sample collected using an automated sampler." ;
    skos:historyNote "Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV." ;
    skos:inScheme cs: ;
    skos:prefLabel "Automated" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/biota> a skos:Concept ;
    skos:definition "A specimen consisting of all or part of a biological organism, including flora or fauna." ;
    skos:inScheme cs: ;
    skos:prefLabel "Biota" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/core> a skos:Concept ;
    skos:definition "Long cylindrical cores" ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Core" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/coreHalfRound> a skos:Concept ;
    skos:definition "Half-cylindrical products of along-axis split of a whole round" ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Core half round" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/corePiece> a skos:Concept ;
    skos:definition "Material occurring between unambiguous [as curated] breaks in recovery." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Core piece" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/coreQuarterRound> a skos:Concept ;
    skos:definition "Quarter-cylindrical products of along-axis split of a half round." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Core quarter round" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/coreSection> a skos:Concept ;
    skos:definition "Arbitrarily cut segments of a \"core\"" ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Core section" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/coreSectionHalf> a skos:Concept ;
    skos:definition "Half-cylindrical products of along-axis split of a section or its component fragments through a selected diameter." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Core section half" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/coreSub-Piece> a skos:Concept ;
    skos:definition "Unambiguously mated portion of a larger piece noted for curatorial management of the material." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Core sub-piece" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/coreWholeRound> a skos:Concept ;
    skos:definition "Cylindrical segments of core or core section material." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Core whole round" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/cuttings> a skos:Concept ;
    skos:definition "Loose, coarse, unconsolidated material suspended in drilling fluid." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Cuttings" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/dredge> a skos:Concept ;
    skos:definition "A group of rocks collected by dragging a dredge along the seafloor." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Dredge" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/foliageDigestion> a skos:Concept ;
    skos:definition "Sample that consists of a digestion of plant foliage" ;
    skos:historyNote "Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV." ;
    skos:inScheme cs: ;
    skos:prefLabel "Foliage digestion" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/foliageLeaching> a skos:Concept ;
    skos:definition "Sample that consists of leachate from foliage" ;
    skos:historyNote "Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV." ;
    skos:inScheme cs: ;
    skos:prefLabel "Foliage leaching" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/forestFloorDigestion> a skos:Concept ;
    skos:definition "Sample that consists of a digestion of forest floor material" ;
    skos:historyNote "Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV." ;
    skos:inScheme cs: ;
    skos:prefLabel "Forest floor digestion" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/grab> a skos:Concept ;
    skos:definition "A sample (sometimes mechanically collected) from a deposit or area, not intended to be representative of the deposit or area." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Grab" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/individualSample> a skos:Concept ;
    skos:definition "A sample that is an individual unit, including rock hand samples, a biological specimen, or a bottle of fluid." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Individual sample" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/litterFallDigestion> a skos:Concept ;
    skos:definition "Sample that consists of a digestion of litter fall" ;
    skos:historyNote "Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV." ;
    skos:inScheme cs: ;
    skos:prefLabel "Litter fall digestion" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/orientedCore> a skos:Concept ;
    skos:definition "Core that can be positioned on the surface in the same way that it was arranged in the borehole before extraction." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Oriented core" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/other> a skos:Concept ;
    skos:definition "Sample does not fit any of the existing type designations. It is expected that further detailed description of the particular sample be provided." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Other" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/petriDishDryDeposition> a skos:Concept ;
    skos:definition "Sample from dry deposition in a petri dish" ;
    skos:historyNote "Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV." ;
    skos:inScheme cs: ;
    skos:prefLabel "Petri dish (dry deposition)" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/precipitationBulk> a skos:Concept ;
    skos:definition "Sample from bulk precipitation" ;
    skos:historyNote "Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV." ;
    skos:inScheme cs: ;
    skos:prefLabel "Precipitation bulk" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/rockPowder> a skos:Concept ;
    skos:definition "A sample created from pulverizing a rock to powder." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Rock powder" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/standardReferenceSpecimen> a skos:Concept ;
    skos:definition "Standard reference specimen" ;
    skos:historyNote "Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV." ;
    skos:inScheme cs: ;
    skos:prefLabel "Standard reference specimen" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/terrestrialSection> a skos:Concept ;
    skos:definition "A sample of a section of the near-surface Earth, generally in the critical zone." ;
    skos:historyNote "SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object." ;
    skos:inScheme cs: ;
    skos:prefLabel "Terrestrial section" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/theSpecimenTypeIsUnknown> a skos:Concept ;
    skos:definition "The specimen type is unknown" ;
    skos:historyNote "Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV." ;
    skos:inScheme cs: ;
    skos:prefLabel "The specimen type is unknown" ;
    skos:topConceptOf cs: .

<http://vocabulary.odm2.org/specimentype/thinSection> a skos:Concept ;
    skos:definition "Thin section" ;
    skos:inScheme cs: ;
    skos:prefLabel "Thin section" ;
    skos:topConceptOf cs: .

